<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.obitosnn.crm.workbench.dao.TranHistoryDao">

    <delete id="deleteTranHistoryByTranId">

        delete from `tbl_tran_history` where `tranId`=#{tranId}

    </delete>

    <select id="selectTranHistoryListByTranId" resultType="TranHistory">

        select
        `id`,
        `stage`,
        `money`,
        `expectedDate`,
        `createTime`,
        `createBy`
        from `tbl_tran_history` where `tranId`=#{tranId} order by `createTime` desc

    </select>

    <insert id="insert">

        insert into tbl_tran_history(
            `id`,
            `stage`,
            `money`,
            `expectedDate`,
            `createTime`,
            `createBy`,
            `tranId`
        )
        values(
            #{id},
            #{stage},
            #{money},
            #{expectedDate},
            #{createTime},
            #{createBy},
            #{tranId}
        )

    </insert>
  	
</mapper>

































